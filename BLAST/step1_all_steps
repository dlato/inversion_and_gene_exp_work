#####################################################################
# blast with perc_identity = 70, qcovs_perc = 50
# https://toolshed.g2.bx.psu.edu/repository/display_tool?repository_id=d5dd1c5d2070513e&tool_config=database%2Fcommunity_files%2F001%2Frepo_1040%2Ftools%2Fblast_rbh%2Fblast_rbh.xml&changeset_revision=ff0b814c1320
#####################################################################
#run first blast on each genome against the respective db
#MG1655
blastp -num_threads 2 -qcov_perc 50 -perc_identity 70 -query /home/dlato/Queenie_2019_2020/Proteomes/Ecoli_K12_MG1655_chrom_U00096_UP000000625_83333.fasta -db /home/dlato/Queenie_2019_2020/Proteomes/Ecol_proteomes_db_NOT_K12_MG1655/Ecol_proteomes_db_NOT_K12_MG1655_25May20 -out Blast_ecoli_db_NOT_K12_MG1655_qcov50_perc_ident70.tab   -outfmt "6 sstrand" -use_sw_tback
