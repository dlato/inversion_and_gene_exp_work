##################
# running all versions of blast
##################
parallel -j 10 < default_blast_parallel
parallel -j 10 < e001_qcov90_blast_parallel 
parallel -j 10 < e001_qcov95_blast_parallel
parallel -j 10 < 1e6_qcov90_blast_parallel
parallel -j 10 < 1e6_qcov95_blast_parallel
parallel -j 10 < qcov50_perc_ident70_blast_parallel
#
#-------------------------------------------------------------------
#
#####################################################################
# blast with default parameters
#####################################################################
#run first blast on each genome against the respective db
#MG1655
blastp -num_threads 2 -query /home/dlato/Queenie_2019_2020/Proteomes/Ecoli_K12_MG1655_chrom_U00096_UP000000625_83333.fasta -db /home/dlato/Queenie_2019_2020/Proteomes/Ecol_proteomes_db_NOT_K12_MG1655/Ecol_proteomes_db_NOT_K12_MG1655_25May20 -out Blast_ecoli_db_NOT_K12_MG1655_default.tab -outfmt "6 sstrand" -use_sw_tback
#ATCC
blastp -num_threads 2 -query /home/dlato/Queenie_2019_2020/Proteomes/Ecoli_ATCC_25922_NZ_CP009072_UP000001410.fasta -db /home/dlato/Queenie_2019_2020/Proteomes/Ecol_proteomes_db_NOT_ATCC/Ecol_proteomes_db_NOT_ATCC_25May20 -out Blast_ecoli_db_NOT_ATCC_default.tab  -outfmt "6 sstrand" -use_sw_tback
#BW25113
blastp -num_threads 2 -query /home/dlato/Queenie_2019_2020/Proteomes/Ecoli_BW25113_NZ_CP009273_UP000029103.fasta -db /home/dlato/Queenie_2019_2020/Proteomes/Ecol_proteomes_db_NOT_BW25113/Ecol_proteomes_db_NOT_BW25113_25May20 -out Blast_ecoli_db_NOT_BW25113_default.tab  -outfmt "6 sstrand" -use_sw_tback
#DH10B
blastp -num_threads 2 -query /home/dlato/Queenie_2019_2020/Proteomes/Ecoli_K12_DH10B_NC_010473_UP000001689.fasta -db /home/dlato/Queenie_2019_2020/Proteomes/Ecol_proteomes_db_NOT_K12_DH10B/Ecol_proteomes_db_NOT_K12_DH10B_25May20 -out Blast_ecoli_db_NOT_K12_DH10B_default.tab  -outfmt "6 sstrand" -use_sw_tback
#run SECOND blast on each db against each genome
#MG1655
blastp -num_threads 2 -query /home/dlato/Queenie_2019_2020/Proteomes/Ecoli_NOT_K12_MG1655_25May20.fasta  -subject /home/dlato/Queenie_2019_2020/Proteomes/Ecoli_K12_MG1655_chrom_U00096_UP000000625_83333.fasta -out Blast_ecoli_db_NOT_K12_MG1655_reversed_default.tab  -outfmt "6 sstrand" -use_sw_tback
#ATCC
blastp -num_threads 2 -query /home/dlato/Queenie_2019_2020/Proteomes/Ecol_proteomes_db_NOT_ATCC_25May20.fasta -subject /home/dlato/Queenie_2019_2020/Proteomes/Ecoli_ATCC_25922_NZ_CP009072_UP000001410.fasta -out Blast_ecoli_db_NOT_ATCC_reversed_default.tab  -outfmt "6 sstrand" -use_sw_tback
